<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ricoh.printer.sender"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_scanner_app"
        android:label="@string/app_name_scan" >
        <activity
            android:name="com.gso.hogoapi.MainActivity"
            android:label="@string/app_name_scan" >
            <intent-filter><action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity-alias
            android:name="com.gso.hogoapi.mainActivity.print"
            android:enabled="true"
            android:icon="@drawable/icon_print_app"
            android:label="@string/app_name_print"
            android:targetActivity="com.gso.hogoapi.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <!-- PDF vier -->
        <activity
            android:name="com.artifex.mupdf.ChoosePDFActivity"
            android:label="@string/picker_title" >
            <intent-filter>
            </intent-filter>
        </activity>
        <activity
            android:name="com.artifex.mupdf.MuPDFActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="application/vnd.ms-xpsdocument" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="application/pdf" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="application/x-cbz" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="file" />
                <data android:mimeType="*/*" />
                <data android:pathPattern=".*\\.xps" />
                <data android:host="*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="file" />
                <data android:mimeType="*/*" />
                <data android:pathPattern=".*\\.pdf" />
                <data android:host="*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="file" />
                <data android:mimeType="*/*" />
                <data android:pathPattern=".*\\.cbz" />
                <data android:host="*" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.artifex.mupdf.OutlineActivity"
            android:label="@string/outline_title" >
        </activity>
        <!-- End  PDF vier -->


        <!-- Printter -->
        <!-- HOGO CUSTOM START -->
        <activity
            android:name="net.keyring.bookend.activity.HoGoPrintSettingActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateHidden|adjustResize" >
        </activity>
        <activity
            android:name="net.keyring.bookend.activity.HoGoWebBookShelfActivity"
            android:configChanges="keyboardHidden|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.keyring.bookend.activity.HoGoDocumentListActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateHidden|adjustResize" >
            <intent-filter>

            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="behttphogo" />
                <data android:scheme="beinfohogo" />
            </intent-filter>
        </activity>
        <!-- HOGO CUSTOM END -->
        <activity
            android:name="net.keyring.bookend.activity.MainActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateHidden|adjustResize" >
        </activity>
        <activity
            android:name="net.keyring.bookend.activity.RegistFormActivity"
            android:configChanges="keyboardHidden|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.keyring.bookend.activity.WebBookShelfActivity"
            android:configChanges="keyboardHidden|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.keyring.bookend.activity.OptionActivity"
            android:configChanges="keyboardHidden|orientation"
            android:windowSoftInputMode="stateHidden|adjustResize" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.keyring.bookend.activity.StoreListActivity"
            android:configChanges="keyboardHidden|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.co.ricoh.ssdk.sample.app.print.activity.MainActivity"
            android:configChanges="keyboardHidden|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="net.keyring.bookend.service.NewDownloadService"
            android:exported="false"
            android:label="@string/app_name"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="net.keyring.bookend.callback.NewDownloadCallback" />

                <category android:name="category_hogo" />
            </intent-filter>
        </service>
        <service
            android:name="net.keyring.bookend.service.WebBookShelfDlService"
            android:exported="false"
            android:label="@string/app_name"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="net.keyring.bookend.callback.WebBookShelfDlCallback" />

                <category android:name="category_hogo" />
            </intent-filter>
        </service>
        <service
            android:name="net.keyring.bookend.service.ProcessCheckService"
            android:label="@string/app_name"
            android:noHistory="true" >
        </service>
        <!-- End Printter -->
        
        
        <!-- Scanner -->
        
        <!-- End Scanner -->
    </application>

</manifest>